msgid "Operation in Progress"
msgstr "En åtgärd behandlas"

#: ../libepc/shell.c:471
msgid "No details known"
msgstr "Inga detaljer är kända"

#: ../libepc/tls.c:165
msgid "Generating Server Key"
msgstr "Genererar servernyckel"

#: ../libepc/tls.c:166
msgid "This may take some time. Type on the keyboard, move your mouse, or browse the web to generate some entropy."
msgstr "Det här kan ta lite tid. Använd tangentbordet, rör på musen eller surfa på webben för att generera entropidata."

#: ../libepc/tls.c:191
#, c-format
msgid "Cannot create private server key: %s"
msgstr "Kan inte skapa privat servernyckel: %s"

#: ../libepc/tls.c:244
#, c-format
msgid "Cannot import private server key '%s': %s"
msgstr "Kan inte importera privata servernyckeln \"%s\": %s"

#: ../libepc/tls.c:300
#, c-format
msgid "Cannot export private key to PEM format: %s"
msgstr "Kan inte exportera privat nyckel till PEM-format: %s"

#: ../libepc/tls.c:314
#, c-format
msgid "Failed to create private key folder '%s': %s"
msgstr "Misslyckades med att skapa mappen \"%s\" för privata nyckelfiler: %s"

#: ../libepc/tls.c:330
#, c-format
msgid "Failed to create private key file '%s': %s"
msgstr "Misslyckades med att skapa privata nyckelfilen \"%s\": %s"

#: ../libepc/tls.c:344
#, c-format
msgid "Failed to write private key file '%s': %s"
msgstr "Misslyckades med att skriva privata nyckelfilen \"%s\": %s"

#: ../libepc/tls.c:416
#, c-format
msgid "Cannot create self signed server key for '%s': %s"
msgstr "Kan inte skapa självsignerad servernyckel för \"%s\": %s"

#: ../libepc/tls.c:470
#, c-format
msgid "Cannot import server certificate '%s': %s"
msgstr "Kan inte importera servercertifikatet \"%s\": %s"

#: ../libepc/tls.c:525
#, c-format
msgid "Cannot export server certificate to PEM format: %s"
msgstr "Kan inte exportera servercertifikatet till PEM-format: %s"

#: ../libepc/tls.c:539
#, c-format
msgid "Failed to create server certificate folder '%s': %s"
msgstr "Misslyckades med att skapa mappen \"%s\" för servercertifikat: %s"

