#: ../libepc/consumer.c:1079
#, c-format
msgid "Unexpected element: '%s'"
msgstr "Μη προβλεπόμενο στοιχείο: '%s'"

#: ../libepc/publisher.c:659
msgid "Table of Contents"
msgstr "Πίνακας περιεχομένων"

#: ../libepc/publisher.c:693
msgid "Sorry, no resources published yet."
msgstr "Σας ζητάμε συγγνώμη, δεν δημοσιεύτηκαν ακόμη πόροι."

#: ../libepc/publisher.c:2638
msgid "%a of %u on %h"
msgstr "%a από %u σε %h"

#: ../libepc/shell.c:267
#, c-format
msgid "Cannot create Avahi client: %s"
msgstr "Δεν είναι δυνατή η δημιουργία του πελάτη του avahi: %s"

#: ../libepc/shell.c:379
#, c-format
#| msgid "Cannot create Avahi client: %s"
msgid "Cannot create Avahi service browser."
msgstr "Δεν είναι δυνατή η δημιουργία υπηρεσίας περιήγησης Avahi"

#. Translators: This is just a generic default message for a progress bar.
#: ../libepc/shell.c:467
#| msgid "Operation Proceeded"
msgid "Operation in Progress"
msgstr "Λειτουργία σε εξέλιξη"

#: ../libepc/shell.c:471
msgid "No details known"
msgstr "Δεν υπάρχουν γνωστές λεπτομέρειες"

#: ../libepc/tls.c:165
msgid "Generating Server Key"
msgstr "Δημιουργία κλειδιού εξυπηρετητή"

#: ../libepc/tls.c:166
msgid "This may take some time. Type on the keyboard, move your mouse, or browse the web to generate some entropy."
msgstr "Αυτό μπορεί να διαρκέσει λίγο χρόνο. Πληκτρολογήστε στο πληκτρολόγιο, μετακινήστε το ποντίκι σας ή περιηγηθείτε στο δίκτυο για να δημιουργήσετε κάποια εντροπία."

#: ../libepc/tls.c:191
#, c-format
msgid "Cannot create private server key: %s"
msgstr "Δεν είναι δυνατή η δημιουργία του προσωπικού κλειδιού εξυπηρετητή: %s"

#: ../libepc/tls.c:244
#, c-format
msgid "Cannot import private server key '%s': %s"
msgstr "Δεν είναι δυνατή η εισαγωγή προσωπικού κλειδιού εξυπηρετητή '%s': %s"

#: ../libepc/tls.c:300
#, c-format
msgid "Cannot export private key to PEM format: %s"
msgstr "Δεν είναι δυνατή η εξαγωγή προσωπικού κλειδιού σε μορφή PEM: %s"

#: ../libepc/tls.c:314
#, c-format
msgid "Failed to create private key folder '%s': %s"
msgstr "Αποτυχία κατά τη δημιουργία του φακέλου προσωπικού κλειδιού '%s': %s"

#: ../libepc/tls.c:330
#, c-format
msgid "Failed to create private key file '%s': %s"
msgstr "Αποτυχία κατά τη δημιουργία του αρχείου προσωπικού κλειδιού '%s': %s"

#: ../libepc/tls.c:344
#, c-format
msgid "Failed to write private key file '%s': %s"
msgstr "Αποτυχία κατά την εγγραφή του αρχείου προσωπικού κλειδιού '%s': %s"

#: ../libepc/tls.c:416
#, c-format
msgid "Cannot create self signed server key for '%s': %s"
msgstr "Δεν είναι δυνατή η δημιουργία προσωπικού κλειδιού εξυπηρετητή για το '%s': %s"

#: ../libepc/tls.c:470
#, c-format
msgid "Cannot import server certificate '%s': %s"
msgstr "Δεν είναι δυνατή η εισαγωγή πιστοποιητικού εξυπηρετητή '%s': %s"

#: ../libepc/tls.c:525
#, c-format
msgid "Cannot export server certificate to PEM format: %s"
msgstr "Δεν είναι δυνατή η εξαγωγή πιστοποιητικού εξυπηρετητή σε μορφή PEM: %s"

#: ../libepc/tls.c:539
#, c-format
msgid "Failed to create server certificate folder '%s': %s"
msgstr "Αποτυχία κατά τη δημιουργία του φακέλου πιστοποιητικού εξυπηρετητή '%s': %s"

